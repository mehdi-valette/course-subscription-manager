  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch via yarn",
      "request": "launch",
      "runtimeArgs": ["dev"],
      "runtimeExecutable": "yarn",
      "skipFiles": ["<node_internals>/**"],
      "type": "node"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "server: nuxt",
      "outputCapture": "std",
      "cwd": "${workspaceFolder}/app",
      "program": "${workspaceFolder}/app/node_modules/nuxi/bin/nuxi.mjs",
      "args": ["dev"]
    }
  ]
}
